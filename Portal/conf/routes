# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


GET      /                           controllers.PortalApp.index
GET      /login                      controllers.PortalApp.loginPage
POST     /loginprocess               controllers.PortalApp.loginProcess
GET      /logout                     controllers.PortalApp.logout
GET      /unauthorize                controllers.PortalApp.unauthAccess
GET      /dashboard                  controllers.PortalApp.dashboard
POST     /addlogitem                 controllers.PortalApp.addLogItem
GET      /broadcast                  controllers.PortalWebSocket.broadcastLog

# Map static resources from the /public folder to the /assets URL path
#GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
